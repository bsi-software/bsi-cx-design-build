{
  "name": "@bsi-cx/design-build",
  "version": "1.0.1",
  "description": "A webpack config builder to create a design for BSI CX.",
  "private": false,
  "engines": {
    "node": ">=14.16.0"
  },
  "browser": "dist/browser.js",
  "main": "dist/main.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode production",
    "watch": "webpack --config webpack.config.js --mode production --watch",
    "pretest": "npx ./test/install.js",
    "posttest": "npx ./test/uninstall.js",
    "test": "cd test && webpack --config webpack.config.js --mode production --progress"
  },
