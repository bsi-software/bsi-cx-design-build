  "name": "@bsi-cx/design-build",
  "version": "1.4.5",
  "description": "A webpack config builder to create a design for BSI CX.",
  "private": false,
  "engines": {
    "node": ">=14.16.0"
  },
  "browser": "dist/browser.js",
  "main": "dist/main.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode production --progress",
